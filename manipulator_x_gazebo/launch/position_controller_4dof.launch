<?xml version="1.0" ?>
<launch>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find manipulator_x_gazebo)/config/position_controller_4dof.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
  output="screen" ns="/gazebo/robotis/manipulator_x4" args="joint_state_controller
                                                    joint1_position
                                                    joint2_position
                                                    joint3_position
                                                    joint4_position
                                                    grip_joint_position
                                                    grip_joint_sub_position"/>
</launch>
