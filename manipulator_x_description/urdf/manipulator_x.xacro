<?xml version="1.0"?>
<!-- ROBOTIS Manipulator-X -->
<robot name="robotis_manipulator_x" xmlns:xacro="http://www.ros.org/wiki/xacro">
  
  <!-- some parameters -->
  <xacro:property name="pi" value="3.141592654" />

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find manipulator_x_description)/urdf/manipulator_x.gazebo" />
  <!-- Import Rviz colors -->
  <xacro:include filename="$(find manipulator_x_description)/urdf/materials.xacro" />

  <!-- World -->
  <link name="world">
  </link>

  <!-- World fixed joint-->
  <joint name="world_fixed" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="world"/>
    <child link="base"/>
  </joint>

  <!-- Base Plate -->
  <link name="base">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 ${-0.5*pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_base.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 ${-0.5*pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_base.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>

  <!-- Joint 1 -->
  <joint name="joint1" type="revolute">
    <parent link="base"/>
    <child link="link2"/>
    <origin xyz="-0.021 0.0 0.023" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit velocity="6.5" effort="1000" lower="${-8.0*pi/9.0}" upper="${8.0*pi/9.0}" />
    <dynamics damping="0.1"/>
  </joint>

  <transmission name="tran1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint1">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor1">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- Link 2 -->
  <link name="link2">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 ${-0.5*pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_0.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 ${-0.5*pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_0.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>

  <!-- Joint 2 -->
  <joint name="joint2" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0.0 0.0 0.0495" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="6.5" effort="1000" lower="${-8.0*pi/9.0}" upper="${8.0*pi/9.0}" />
    <dynamics damping="0.1"/>
  </joint>

  <transmission name="tran2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint2">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor2">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- Link 3 -->
  <link name="link3">
    <collision>
      <origin xyz="0 0 0" rpy="${-0.5*pi} 0 ${pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="${-0.5*pi} 0 ${pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>

  <!-- Joint 3 -->
  <joint name="joint3" type="revolute">
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz="0.029 0.0 0.102" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="6.5" effort="1000" lower="${-8.0*pi/9.0}" upper="${8.0*pi/9.0}" />
    <dynamics damping="0.1"/>
  </joint>

  <transmission name="tran3">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint3">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor3">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- Link 4 -->
  <link name="link4">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 ${-0.5*pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_2.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 ${-0.5*pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_2.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>

  <!-- Joint 4 -->
  <joint name="joint4" type="revolute">
    <parent link="link4"/>
    <child link="link5"/>
    <origin xyz="0.045 0.0 0.024" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit velocity="6.5" effort="1000" lower="${-8.0*pi/9.0}" upper="${8.0*pi/9.0}" />
    <dynamics damping="0.1"/>
  </joint>

  <transmission name="tran4">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint4">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor4">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- Link 5 -->
  <link name="link5">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 ${-0.5*pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_3.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 ${-0.5*pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_3.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>

  <!-- Joint 5 -->
  <joint name="joint5" type="revolute">
    <parent link="link5"/>
    <child link="link6"/>
    <origin xyz="0.0595 0.0 0.0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="6.5" effort="1000" lower="${-8.0*pi/9.0}" upper="${8.0*pi/9.0}" />
    <dynamics damping="0.1"/>
  </joint>

  <transmission name="tran5">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint5">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor5">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- Link 6 -->
  <link name="link6">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 ${-0.5*pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_4.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 ${-0.5*pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_4.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>

  <!-- Joint 6 -->
  <joint name="joint6" type="revolute">
    <parent link="link6"/>
    <child link="link7"/>
    <origin xyz="0.045 0.0 0.0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit velocity="6.5" effort="1000" lower="${-8.0*pi/9.0}" upper="${8.0*pi/9.0}" />
    <dynamics damping="0.1"/>
  </joint>

  <transmission name="tran6">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint6">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor6">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- Link 7 -->
  <link name="link7">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 ${-0.5*pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_5.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 ${-0.5*pi}"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/assm_mpx540_6x_5.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>

</robot>
