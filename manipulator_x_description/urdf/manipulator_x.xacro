    <inertial>
      <origin xyz="0.00025 0.00000 0.01780" rpy="0 0 0"/>
      <mass value="0.08659"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      <!--<inertia ixx="0.00001371" ixy="0.0" ixz="-0.00000025" iyy="0.00002369" iyz="0.0" izz="0.00002097" />-->
    </inertial>
  </link>

  <!-- Joint 1 -->
  <joint name="joint1" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0.012 0.0 0.036" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit velocity="6.5" effort="1000" lower="${-8.0*pi/9.0}" upper="${8.0*pi/9.0}" />
    <dynamics damping="0.7"/>
  </joint>

  <transmission name="tran1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint1">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor1">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

