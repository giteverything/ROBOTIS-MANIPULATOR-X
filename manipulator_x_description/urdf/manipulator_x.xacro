<?xml version="1.0"?>
<!-- ROBOTIS Manipulator-X -->
<robot name="robotis_manipulator_x" xmlns:xacro="http://www.ros.org/wiki/xacro">
  
  <!-- some parameters -->
  <xacro:property name="pi" value="3.141592654" />

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find manipulator_x_description)/urdf/manipulator_x.gazebo" />
  <!-- Import Rviz colors -->
  <xacro:include filename="$(find manipulator_x_description)/urdf/materials.xacro" />

  <!-- World -->
  <link name="world">
  </link>

  <!-- World fixed joint-->
  <joint name="world_fixed" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="world"/>
    <child link="base"/>
  </joint>

  <!-- Base Plate -->
  <link name="base">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_base.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_base.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
  </link>
  
    <!-- Base fixed joint -->
  <joint name="base_fixed" type="fixed">
    <parent link="base"/>
    <child link="link1"/>
  </joint>

  <!-- Link 1 -->
  <link name="link1">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0.00027 0.00000 0.01779" rpy="0 0 0"/>
      <mass value="0.08581"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      <!--<inertia ixx="0.00001360" ixy="0.0" ixz="-0.00000022" iyy="0.00002352" iyz="0.0" izz="0.00002080" />-->
    </inertial>
  </link>
  
    <!-- Joint 1 -->
  <joint name="joint1" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0.012 0.0 0.034" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
    <dynamics damping="0.7"/>
  </joint>

  <transmission name="tran1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint1">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor1">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  
    <!-- Link 2 -->
  <link name="link2">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link2.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link2.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0.00000 0.00000 0.00975" rpy="0 0 0"/>
      <mass value="0.00795"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />      
      <!--<inertia ixx="0.00000265" ixy="0.0" ixz="0.0" iyy="0.00000105" iyz="0.0" izz="0.00000246" />-->
    </inertial>
  </link>
  
  <!-- Joint 2 -->
  <joint name="joint2" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0.0 -0.017 0.03" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
    <dynamics damping="0.7"/>
  </joint>

  <transmission name="tran2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint2">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor2">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  
  <!-- Link 3 -->
  <link name="link3">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link3.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link3.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0.00535 0.01652 0.05937" rpy="0 0 0"/>
      <mass value="0.21941"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      <!--<inertia ixx="0.00043395" ixy="0.00000013" ixz="-0.00005129" iyy="0.00044404" iyz="-0.00000018" izz="0.00005415" />-->
    </inertial>
  </link>
  
  <!-- Joint 3 -->
  <joint name="joint3" type="revolute">
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz="0.024 0.0 0.1045" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
    <dynamics damping="0.7"/>
  </joint>

  <transmission name="tran3">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint3">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor3">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <!-- Link 4 -->
  <link name="link4">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link4.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link4.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0.04298 0.01700 0.01188" rpy="0 0 0"/>
      <mass value="0.09746"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />      
      <!--<inertia ixx="0.00002580" ixy="0.0" ixz="-0.00000144" iyy="0.00003203" iyz="0.0" izz="0.00002291" />-->
    </inertial>
  </link>  
  
  <!-- Joint 4 -->
  <joint name="joint4" type="revolute">
    <parent link="link4"/>
    <child link="link5"/>
    <origin xyz="0.062 0.017 0.024" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit velocity="6.5" effort="1000" lower="${-5.0*pi/6.0}" upper="${5.0*pi/6.0}" />
    <dynamics damping="0.7"/>
  </joint>

  <transmission name="tran4">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint4">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor4">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  
  <!-- Link 5 -->
  <link name="link5">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link5.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link5.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0.02929 -0.00057 0.00000" rpy="0 0 0"/>
      <mass value="0.09226"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />      
      <!--<inertia ixx="0.00001535" ixy="0.00000012" ixz="0.0" iyy="0.00002498" iyz="0.0" izz="0.00002865" />-->
    </inertial>
  </link>  
  
  <!-- Joint 5 -->
  <joint name="joint5" type="revolute">
    <parent link="link5"/>
    <child link="link6"/>
    <origin xyz="0.0425 -0.017 0.0" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
    <dynamics damping="0.7"/>
  </joint>

  <transmission name="tran5">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint5">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor5">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  
  <!-- Link 6 -->
  <link name="link6">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link6.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link6.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0.04298 0.01700 0.01140" rpy="0 0 0"/>
      <mass value="0.09746"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />      
      <!--</><inertia ixx="0.00002577" ixy="0.0" ixz="-0.00000087" iyy="0.00003200" iyz="0.0" izz="0.00002291" />-->
    </inertial>
  </link>  
  
  <!-- Joint 6 -->
  <joint name="joint6" type="revolute">
    <parent link="link6"/>
    <child link="link789"/>
    <origin xyz="0.062 0.017 0.0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit velocity="6.5" effort="1000" lower="${-5.0*pi/6.0}" upper="${5.0*pi/6.0}" />
    <dynamics damping="0.7"/>
  </joint>

  <transmission name="tran6">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint6">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor6">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  
  <!-- Link 789 -->
  <link name="link789">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link789.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator_x_description/meshes/manipulator_x_link789.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="0.04197 0.00146 -0.00021" rpy="0 0 0"/>
      <mass value="0.26121"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />      
      <!--<inertia ixx="0.00019" ixy="0.00001" ixz="0.0" iyy="0.00022" iyz="0.0" izz="0.00029" />-->
    </inertial>
  </link>  
  
</robot>
